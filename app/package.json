{
  "name": "art-browser",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_docker": "ng serve --configuration=docker",
    "start:fr": "ng serve --base-href /fr/ --configuration=fr",
    "start:de": "ng serve --base-href /de/ --configuration=de",
    "start:en": "ng serve --base-href /en/ --configuration=en",
    "start:it": "ng serve --base-href /it/ --configuration=it",
    "start:es": "ng serve --base-href /es/ --configuration=es",
    "build": "ng build",
    "build-locale": "npm run build:en && npm run build:de && npm run build:fr && npm run build:es && npm run build:it",
    "build:fr": "ng build --base-href /fr/ --configuration=fr-production",
    "build:de": "ng build --base-href /de/ --configuration=de-production",
    "build:en": "ng build --base-href /en/ --configuration=en-production",
    "build:it": "ng build --base-href /it/ --configuration=it-production",
    "build:es": "ng build --base-href /es/ --configuration=es-production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "locale": "ng xi18n --output-path locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ng-bootstrap/ng-bootstrap": "^5.1.4",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "core-js": "^3.4.7",
    "lodash": "^4.17.15",
    "ngx-image-viewer": "github:hochschule-darmstadt/ngx-image-viewer#build",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.20",
    "@angular/cli": "~8.3.20",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.0",
    "eslint": "~6.7.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.19.4",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.20.1",
    "typescript": "~3.4.5"
  }
}
