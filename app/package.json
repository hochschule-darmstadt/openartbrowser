{
  "name": "art-browser",
  "version": "1.2.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_docker": "ng serve --configuration=docker",
    "start:fr": "ng serve --base-href /fr/ --configuration=fr --live-reload",
    "start:de": "ng serve --base-href /de/ --configuration=de --live-reload",
    "start:en": "ng serve --base-href /en/ --configuration=en --live-reload",
    "start:it": "ng serve --base-href /it/ --configuration=it --live-reload",
    "start:es": "ng serve --base-href /es/ --configuration=es --live-reload",
    "build": "ng build",
    "build-locale": "npm run build:en && npm run build:de && npm run build:fr && npm run build:es && npm run build:it",
    "build:fr": "ng build --base-href /fr/ --configuration=fr-production",
    "build:de": "ng build --base-href /de/ --configuration=de-production",
    "build:en": "ng build --base-href /en/ --configuration=en-production",
    "build:it": "ng build --base-href /it/ --configuration=it-production",
    "build:es": "ng build --base-href /es/ --configuration=es-production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "locale": "ng xi18n --output-path locale"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics-cli": "^12.0.4",
    "@angular-slider/ngx-slider": "^2.0.3",
    "@angular/animations": "^14.3.0",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/localize": "^14.3.0",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@ng-bootstrap/ng-bootstrap": "^11.0.0",
    "@types/element-resize-detector": "^1.1.2",
    "@types/lodash": "^4.14.168",
    "@types/qs": "^6.9.6",
    "angulartics2": "^10.0.0",
    "bodybuilder": "^2.3.0",
    "core-js": "^3.10.1",
    "element-resize-detector": "^1.2.1",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.21",
    "ng-in-viewport": "^6.1.0",
    "ngx-image-viewer": "https://github.com/hochschule-darmstadt/ngx-image-viewer#build",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.13",
