{
  "name": "art-browser",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start_docker": "ng serve --configuration=docker",
    "start:fr": "ng serve --base-href /fr/ --configuration=fr",
    "start:de": "ng serve --base-href /de/ --configuration=de",
    "start:en": "ng serve --base-href /en/ --configuration=en",
    "start:it": "ng serve --base-href /it/ --configuration=it",
    "start:es": "ng serve --base-href /es/ --configuration=es",
    "build": "ng build",
    "build-locale": "npm run build:en && npm run build:de && npm run build:fr && npm run build:es npm run build:it",
    "build:fr": "ng build --base-href /fr/ --configuration=fr ",
    "build:de": "ng build --base-href /de/ --configuration=de",
    "build:en": "ng build --base-href /en/ --configuration=en",
    "build:it": "ng build --base-href /it/ --configuration=it",
    "build:es": "ng build --base-href /es/ --configuration=es",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "locale": "ng xi18n --output-path locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/common": "^7.2.15",
    "@angular/compiler": "^7.2.15",
    "@angular/core": "^7.2.15",
    "@angular/forms": "^7.2.15",
    "@angular/platform-browser": "^7.2.15",
    "@angular/platform-browser-dynamic": "^7.2.15",
    "@angular/router": "^7.2.15",
    "@ng-bootstrap/ng-bootstrap": "^4.2.2",
    "@types/lodash": "^4.14.144",
    "@types/qs": "^6.5.3",
    "core-js": "^2.6.10",
    "lodash": "^4.17.15",
    "ngx-image-viewer": "github:hochschule-darmstadt/ngx-image-viewer#build",
    "rxjs": "~6.3.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "^7.2.15",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon": "^1.19.4",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  }
